<action>
	<icon>cm_extractfiles</icon>
	<name>Extract to folder (fixme)</name>
	<submenu></submenu>
	<unique-id>1681328989848422-1</unique-id>
	<command>/bin/bash -c &apos;archive_path=&quot;%f&quot;; base_name=&quot;$(basename &quot;$archive_path&quot; | sed -E &quot;s/\.[^.]+$//&quot;)&quot;; dir_name=&quot;$(dirname &quot;$archive_path&quot;)&quot;; mkdir -p &quot;$dir_name/$base_name&quot;; file-roller --extract-to=&quot;$dir_name/$base_name&quot; &quot;$archive_path&quot;&apos;</command>
	<description></description>
	<range>*</range>
	<patterns>*.zip;*.tar;*.tar.gz;*.tgz;*.tar.xz;*.txz;*.tar.bz2;*.tbz2;*.rar;*.7z</patterns>
	<other-files/>
</action>
